    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <View
            android:id="@+id/view"
            android:layout_width="match_parent"
            android:layout_height="8dp"
            android:background="#f3f3f3" />
    <LinearLayout
        android:id="@+id/ll_tuiguang"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical|bottom"
        android:orientation="horizontal"
        android:paddingBottom="10dp"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:paddingTop="10dp" >


        <TextView
            android:id="@+id/tv_ketimoney"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_marginLeft="5dp"
            android:textStyle="bold"
            android:textSize="16dp"
            android:textColor="@color/tuiguang_color3"
            android:text="可提现金额0.0" />


        <TextView
            android:id="@+id/tv_tuiguang_tule"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="立即提现"
            android:gravity="center"
            android:textSize="12dp"
            android:paddingBottom="5dp"
            android:paddingTop="4dp"
            android:paddingRight="10dp"
            android:paddingLeft="10dp"
            android:textColor="@color/tuiguang_color2"
            android:background="@drawable/bg_tuiguang" />
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:visibility="visible"
        android:background="@color/color_line"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <LinearLayout
            android:id="@+id/ll_brokerage"
            android:background="@drawable/btn_seclecter"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:gravity="center"
            android:paddingBottom="15dp"
            android:paddingTop="15dp" >
            <TextView
                android:id="@+id/tv_lastmonth"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textSize="16dp"
                android:textColor="@color/tuiguang_color3"
                android:text="¥0.0" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/shop_color1"
                    android:textSize="14dp"
                    android:layout_marginTop="10dp"
                    android:text="上月结算"/>
        </LinearLayout>
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:visibility="gone"
            android:background="@color/color_line"/>
        <LinearLayout
            android:id="@+id/ll_fanli_order"
            android:layout_weight="1"
            android:background="@drawable/btn_seclecter"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:gravity="center"
            android:paddingBottom="15dp"
            android:paddingTop="15dp">
            <TextView
                android:id="@+id/tv_thismonth"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textSize="16dp"
                android:textColor="@color/tuiguang_color3"
                android:text="¥0.0" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/shop_color1"
                    android:textSize="14dp"
                    android:layout_marginTop="10dp"
                    android:text="本月结算"/>
        </LinearLayout>
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:visibility="gone"
            android:background="@color/color_line"/>
        <LinearLayout
            android:id="@+id/ll_benyueyugu"
            android:layout_weight="1"
            android:background="@drawable/btn_seclecter"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:gravity="center"
            android:paddingBottom="15dp"
            android:paddingTop="15dp">
            <TextView
                android:id="@+id/tv_thismonthyu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textSize="16dp"
                android:textColor="@color/tuiguang_color3"
                android:text="¥0.0" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/shop_color1"
                    android:textSize="14dp"
                    android:layout_marginTop="10dp"
                    android:text="本月预估"/>
        </LinearLayout>
    </LinearLayout>
    </LinearLayout>


    <RelativeLayout
        android:id="@+id/ll_hongbao"
        android:layout_width="55dp"
        android:layout_marginLeft="175dp"
        android:layout_height="wrap_content">

        <ImageView
            android:id="@+id/img_hongbao"
            android:layout_width="55dp"
            android:layout_height="60dp"
            android:background="@mipmap/hongbao_01"/>

        <TextView
            android:id="@+id/hongbao_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:background="@drawable/text_result_red_more"
            android:paddingLeft="4dp"
            android:paddingRight="4dp"
            android:layout_marginTop="5dp"
            android:text=""
            android:textColor="#ffffff"
            android:textSize="12dp" />
    </RelativeLayout>
</RelativeLayout>